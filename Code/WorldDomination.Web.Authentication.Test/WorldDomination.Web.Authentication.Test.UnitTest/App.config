<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="authenticationProviders"
             type="WorldDomination.Web.Authentication.Config.ProviderConfiguration, WorldDomination.Web.Authentication" />
  </configSections>

  <authenticationProviders callbackUri="http://www.mywebsite.com/authenticationCallback">
    <providers>
      <add name="Facebook" key="testKey" secret="testSecret" />
      <add name="Google" key="googleKey" secret="googleSecret" />
    </providers>
  </authenticationProviders>

</configuration>